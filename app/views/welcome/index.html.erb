<div class="jumbotron">
  		<h1>Welcome To Blink!</h1>

	<p>The largest social media community in the world.</p>
	

	<div class="front_page_sign_up">
			<div class="col-md-12">
				<div class="panel panel-default">
					<div class="panel-body">
						<form role="form">
							<div class="form-group">
								<label for="First Name">First Name</label>
								<input type="First Name" class="form-control" id="FirstNameInput" placeholder="First Name">
							</div>						
							<div class="form-group">
								<label for="exampleInputLastName">Last Name</label>
								<input type="Last Name" class="form-control" id="exampleInputLastName1" placeholder="Last Name">
							</div>
						  	<div class="form-group">
						   	 <label for="exampleInputEmail1">Email address</label>
						    <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email">
						  	</div>
						  	<div class="form-group">
						   	 <label for="exampleInputPassword1">Password</label>
						    	<input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
						 	 </div>
						  	<div class="form-group">
						    	<label for="exampleInputPassword1">Confirm Password</label>
						   	 <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Confirm Password">
						  	</div>
						  	<div class="checkbox">
						    	<label>
						      <input type="checkbox"> Remember Me
						    	</label>
						  	</div>
						  <button type="submit" class="btn btn-default"><%= link_to "Sign Up!", new_user_registration_path %></button>
						</form>
					</div>
				</div>
			</div>	
	</div>
	<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>
</div>